help = build pd by repo address, then use the binary to patch cluster
quiet-err = true

[arg]
git-repo|repo = https://github.com/pingcap/pd
git-branch|branch = ''
git-hash|hash = ''
cluster-name|cluster|name|n = ''

[arg2env]
tidb.cluster = cluster-name

[env]
tidb.cluster = read

[dep]
make = to build pd
go = to build pd
