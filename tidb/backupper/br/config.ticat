help = setup br config, set self as tidb.backupper
tags = config
abbr = conf

[arg]
backup-dir|dir|d = ''
threads|thread|t|T = ''
cluster-name|cluster|name|n|N = ''
br-bin = 'tiup br'

[arg2env]
br.backup-dir = backup-dir
br.threads = threads
tidb.cluster = cluster-name
br.bin = br-bin

[val2env]
tidb.backupper = tidb.backupper.br
