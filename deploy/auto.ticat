help = tidb auto deployer
trivial = 1

[arg]
hosts = ''
deploy-user|user = tidb
allocator = deploy.auto.allocator.tom

[arg2env]
deploy.hosts = hosts
deploy.user = deploy-user

[flow/]
deploy.host.auth
deploy.host.hardware-resource
deploy.host.idle-scan
deploy.host.auto-mount
[[allocator]]
deploy.set.port.delta 1000
deploy.gen.tiup
