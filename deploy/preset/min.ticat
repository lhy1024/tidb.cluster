help = 1*tikv + 1*tidb + 1*pd on the same host
trivial = 3

[arg]
port-delta|port|p = +0
host|h = 127.0.0.1
cluster-name|cluster|name|n = test

[flow/]
tidb.config [[cluster-name]]
deploy.set.hosts.tikv  [[host]]
deploy.set.hosts.pd    [[host]]
deploy.set.hosts.tidb  [[host]]
deploy.set.ports delta=[[port-delta]]
